{
  "rewrites": [
    {
      "source": "/api/inngest",
      "destination": "/api/inngest.js"
    },
    {
      "source": "/api/token", 
      "destination": "/api/token.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}